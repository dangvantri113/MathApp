<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mycompany.mathapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListActivity" />
        <activity android:name=".NumberActivity" />
        <activity android:name=".OnboardActivity" />
        <activity android:name=".ResumeActivity" />
        <activity android:name=".ShapeActivity" />
        <activity android:name=".StudyActivity" />
        <activity android:name=".TimeActivity" />
        <activity android:name=".TriangleActivity" />
        <activity android:name=".IsTriangleActivity"></activity>
    </application>

</manifest>